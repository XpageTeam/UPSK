header.head
	.head__wrap
		.head__logo
			a.logo(href="#")
				img(src="img/logo.svg")
		nav.head__menu
			ul.h-menu
				each link, i in ["Продукция", "Логистика", "О компании", "Полезное", "Контакты", ]
					li
						if (i == 0)
							a.h-menu__link.active(href="#")=link
						else
							a.h-menu__link(href="#")=link
		.head__search
			a.search-link(href="#")
		.head__contacts
			.h-contacts
				address.h-contacts__phone
					a(href="tel: 8 351 211 00 70") 8 351 211 00 70
				.h-contacts__btn
					a.default-btn.fancybox(href="#") Заказать звонок